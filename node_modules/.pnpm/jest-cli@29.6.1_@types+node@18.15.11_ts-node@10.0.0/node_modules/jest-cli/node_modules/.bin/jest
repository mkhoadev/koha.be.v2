#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/khoadev/Documents/MyWeb/koha.be.v2/node_modules/.pnpm/jest-cli@29.6.1_@types+node@18.15.11_ts-node@10.0.0/node_modules/jest-cli/bin/node_modules:/Users/khoadev/Documents/MyWeb/koha.be.v2/node_modules/.pnpm/jest-cli@29.6.1_@types+node@18.15.11_ts-node@10.0.0/node_modules/jest-cli/node_modules:/Users/khoadev/Documents/MyWeb/koha.be.v2/node_modules/.pnpm/jest-cli@29.6.1_@types+node@18.15.11_ts-node@10.0.0/node_modules:/Users/khoadev/Documents/MyWeb/koha.be.v2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/khoadev/Documents/MyWeb/koha.be.v2/node_modules/.pnpm/jest-cli@29.6.1_@types+node@18.15.11_ts-node@10.0.0/node_modules/jest-cli/bin/node_modules:/Users/khoadev/Documents/MyWeb/koha.be.v2/node_modules/.pnpm/jest-cli@29.6.1_@types+node@18.15.11_ts-node@10.0.0/node_modules/jest-cli/node_modules:/Users/khoadev/Documents/MyWeb/koha.be.v2/node_modules/.pnpm/jest-cli@29.6.1_@types+node@18.15.11_ts-node@10.0.0/node_modules:/Users/khoadev/Documents/MyWeb/koha.be.v2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
